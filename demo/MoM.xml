    <Content type="html">
        <![CDATA[
        <!DOCTYPE HTML>
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
        <html lang="en">
        <head>
            <title>Nivo Slider Demo</title>
            <link rel="stylesheet" href="https://github.com/kd-n/SDT1-MoM/blob/master/themes/default/default.css" type="text/css" media="screen" />
            <link rel="stylesheet" href="https://github.com/kd-n/SDT1-MoM/blob/master/themes/light/light.css" type="text/css" media="screen" />
            <link rel="stylesheet" href="https://github.com/kd-n/SDT1-MoM/blob/master/themes/dark/dark.css" type="text/css" media="screen" />
            <link rel="stylesheet" href="https://github.com/kd-n/SDT1-MoM/blob/master/themes/bar/bar.css" type="text/css" media="screen" />
            <link rel="stylesheet" href="https://github.com/kd-n/SDT1-MoM/blob/master/nivo-slider.css" type="text/css" media="screen" />
            <link rel="stylesheet" href="https://github.com/kd-n/SDT1-MoM/blob/master/demo/style.css" type="text/css" media="screen" />
            <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/jquery-nivoslider/3.2/nivo-slider.css" type="text/css" media="screen" />     
            <style>
                #slider > .nivo-slice {height:100% !important; }
            </style>
        </head>
        <body>
            <div id="wrapper">
              <div class="slider-wrapper theme-default">
              
                <div id="slider" class="nivoSlider">
                <img src="https://github.com/kd-n/SDT1-MoM/blob/master/demo/images/Mina.JPG" data-thumb="https://github.com/kd-n/SDT1-MoM/blob/master/demo/images/Mina.JPG" alt="" title="June 2017"/>
                <img src="https://github.com/kd-n/SDT1-MoM/blob/master/demo/images/Mina.JPG" data-thumb="https://github.com/kd-n/SDT1-MoM/blob/master/demo/images/Mina.JPG" alt="" data-transition="slideInLeft" title="May 2017" />
                <img src="https://github.com/kd-n/SDT1-MoM/blob/master/demo/images/Mark.JPG" data-thumb="https://github.com/kd-n/SDT1-MoM/blob/master/demo/images/Mark.JPG" alt="" data-transition="slideInLeft" title="March 2017" />
                <img src="https://github.com/kd-n/SDT1-MoM/blob/master/demo/images/Mark.JPG" data-thumb="https://github.com/kd-n/SDT1-MoM/blob/master/demo/images/Mark.JPG" alt="" data-transition="slideInLeft" title="February 2017" />
                <img src="https://github.com/kd-n/SDT1-MoM/blob/master/demo/images/Mark.JPG" data-thumb="https://github.com/kd-n/SDT1-MoM/blob/master/demo/images/Mark.JPG" alt="" data-transition="slideInLeft" title="January 2017" />
                <img src="https://github.com/kd-n/SDT1-MoM/blob/master/demo/images/Pau.JPG" data-thumb="https://github.com/kd-n/SDT1-MoM/blob/master/demo/images/Pau.JPG" alt="" data-transition="slideInLeft" title="December 2016"/>
                </div>
             </div>
            </div>
            <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.js"></script>
            <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery-nivoslider/3.2/jquery.nivo.slider.js"></script>
            <script type="text/javascript">
                $(window).on('load', function () {
                    $('#slider').nivoSlider();
                });
            </script>
        </body>
    </html>
